        "@riotjs/webpack-loader": "^10.0.0",
        "@types/chai": "^5.2.2",
        "@types/node": "^24.3.0",
        "c8": "^10.1.3",
        "chai": "^6.0.1",
        "copy-webpack-plugin": "^13.0.1",
        "html-webpack-plugin": "^5.6.4",
        "jsdom": "^26.1.0",
        "jsdom-global": "^3.0.2",
        "mocha": "^11.7.1",
        "prettier": "^3.6.2",
        "ts-loader": "^9.5.4",
        "typescript": "^5.9.2",
        "webpack": "^5.101.3",
        "webpack-cli": "^6.0.1",
