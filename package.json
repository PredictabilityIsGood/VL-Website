{
    "name": "vl-website",
    "version": "1.0.0",
    "description": "Vault Lambda Website",
    "main": "index.js",
    "scripts": {
        "test": "c8 mocha --require test-setup.js src/**/*.spec.js",
        "cov": "c8 report --reporter=text-lcov",
        "cov-html": "c8 report --reporter=html",
        "build": "webpack --mode production",
